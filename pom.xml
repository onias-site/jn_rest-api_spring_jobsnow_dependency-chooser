		</dependency>
		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_http_apache-mime</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_main-authentication_gcp-oauth</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_rest-api-handler-exception_spring</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>

		<dependency>
			<groupId>org.springdoc</groupId>
			<artifactId>springdoc-openapi-starter-webmvc-ui</artifactId>
			<version>${springdoc.version}</version>
		</dependency>
		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_mocking_jobsnow</artifactId>
			<version>0.0.1-SNAPSHOT</version>

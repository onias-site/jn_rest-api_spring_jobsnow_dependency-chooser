		</dependency>
		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_mensageria-sender_gcp-pubsub</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.ccp</groupId>
			<artifactId>ccp_password_mindrot</artifactId>
			<version>0.0.1-SNAPSHOT</version>
		</dependency>
		<dependency>
